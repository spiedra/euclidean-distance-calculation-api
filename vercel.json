{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "files": [
        { 
            "input": "./data.xlsx",
            "output": "data.xlsx"
        }
    ]
}